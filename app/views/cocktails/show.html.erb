<h1><%= @cocktail.name  %></h1>
    <%= image_tag @cocktail.photo.url, height: 300, width: 400 %>
<br>
<h4>Description</h4>
  <% @cocktail.doses.each do |dose| %>
    <%= dose.ingredient.name %>
    <%= dose.description %>
<% end %>
<br>
<br>
<p>
  <%= link_to "Add a dose", new_cocktail_dose_path(@cocktail) %>
</p>

